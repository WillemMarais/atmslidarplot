{
    /* ************************* */
    /* The geophysical variables */
    /* ************************* */
    "particulate_backscatter": {
        "long_name": "particulate backscatter",
        "units": "1/{range_unit} 1/sr",
        "__hvplot__": {
            "default": {
                "call": {
                    "x": "{time_axis}",
                    "y": "{range_axis}"
                },
                "opts": {
                    "title": "particulate backscatter {label} {group} {dimensions}"
                }
            },
            "quadmesh": {
                "call": {
                    "cmap": "turbo",
                    "rasterize": true,
                    "aggregator": "mean",
                    "logz": true,
                    "clabel": "particulate backscatter [1/{range_unit} 1/sr]"
                }
            }
        }
    },
